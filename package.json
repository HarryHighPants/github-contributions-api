{
  "name": "minimal-github-contribution-api",
  "description": "Scrape minimal contribution data of a Github user",
  "version": "4.7.2",
  "author": "Harry High Pants <hey@harryab.com>",
  "license": "MIT",
  "private": true,
  "main": "api/server.ts",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "nodemon api/server.ts --exec ts-node api/server.ts",
    "format": "prettier --write .",
    "prepare": "husky",
    "start": "ts-node api/server.ts",
    "test": "jest",
    "typecheck": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harryhighpants/github-contributions-api.git"
  },
  "bugs": {
    "url": "git+https://github.com/harryhighpants/github-contributions-api.git"
  },
  "keywords": [
