    "compression": "^1.7.4",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "memory-cache": "^0.2.0",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "rotating-file-stream": "^2.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-typescript": "^7.14.5",
