    "contributions",
    "api",
    "node",
    "minimal"
  ],
  "dependencies": {
    "cheerio": "^1.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.4",
    "domhandler": "^5.0.3",
    "express": "^4.19.2",
    "rotating-file-stream": "^3.2.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-typescript": "^7.24.7",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.2.0",
    "@types/supertest": "^6.0.2",
    "husky": "^9.1.4",
    "jest": "^29.6.1",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
