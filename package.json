    "cheerio": "^0.22.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "memory-cache": "^0.2.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cheerio": "^0.22.23",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/lodash": "^4.14.166",
    "@types/memory-cache": "^0.2.1",
    "@types/node-fetch": "^2.5.7",
