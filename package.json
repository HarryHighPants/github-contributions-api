    "express": "^4.16.3",
    "memory-cache": "^0.2.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cheerio": "^0.22.28",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/lodash": "^4.14.169",
    "@types/memory-cache": "^0.2.1",
