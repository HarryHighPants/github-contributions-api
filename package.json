    "@types/morgan": "^1.9.3",
    "@types/node-fetch": "^2.5.12",
    "@types/supertest": "^2.0.11",
    "babel-jest": "^27.0.6",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "supertest": "^6.1.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
